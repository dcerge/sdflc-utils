{
  "name": "@sdflc/utils",
  "version": "1.0.19",
  "description": "A set of utilites used in the @sdflc libraries and related projects. Purpose is to minimize usage of 3rd party libraries.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "build": "rimraf ./build && tsc",
    "lint": "eslint . --ext .ts --fix",
    "test": "jest --config jest.config.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install && npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Sergey Demidenko",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.18.12",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcerge/sdflc-utils.git"
  },
  "keywords": [
    "utils",
    "sdflc"
  ],
  "bugs": {
    "url": "https://github.com/dcerge/sdflc-utils/issues"
  },
  "homepage": "https://github.com/dcerge/sdflc-utils#readme"
}
