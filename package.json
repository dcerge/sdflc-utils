{
  "name": "@sdflc/utils",
  "version": "1.0.19",
  "description": "A set of utilites used in the @sdflc libraries and related projects. Purpose is to minimize usage of 3rd party libraries.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "build": "rimraf ./build && tsc",
    "lint": "eslint . --ext .ts --fix",
    "test": "jest --config jest.config.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install && npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Sergey Demidenko",
  "license": "ISC",
  "dependencies": {
    "dayjs": "^1.11.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcerge/sdflc-utils.git"
  },
  "keywords": [
    "utils",
    "sdflc"
  ],
  "bugs": {
    "url": "https://github.com/dcerge/sdflc-utils/issues"
  },
  "homepage": "https://github.com/dcerge/sdflc-utils#readme"
}
